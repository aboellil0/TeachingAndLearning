@using Microsoft.AspNetCore.Identity;
@inject SignInManager<User> signInManager;

@model Register
@if (!signInManager.IsSignedIn(User))
{
    <h1>You are not sigin in</h1>
}
else
{
    <h1>You are sigined in sucsscfully</h1>
    <a class=" text-white btn btn-primary" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
    <a class="text-white btn btn-danger" asp-area="" asp-controller="Account" asp-action="Signout">Sigin out</a>
}