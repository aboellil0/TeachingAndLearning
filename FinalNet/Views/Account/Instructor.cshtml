


@model List<Instuctorclass>

<h1 class="fw-bold">Courses</h1>

<table class="table table-striped mt-5">
    <thead>
        <tr class="fw-bold fs-6">
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>phone number</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="">
                <td>@item.FName</td>
                <td>@item.LName</td>
                <td>@item.Email</td>
                <td>@item.Ph</td>
                <td><a asp-action="InsDetails" asp-route-id="@item.Id" class="btn btn-primary">Details</a></td>
            </tr>
        }
    </tbody>
</table>